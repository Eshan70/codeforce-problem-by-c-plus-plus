#include <bits/stdc++.h>
using namespace std;
 
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
 
    int t;
    cin >> t;
    while (t--) {
        long long n;
        cin >> n;
        vector<long long> results;
 
        for (int k = 1; k <= 18; k++) { // k from 1 to 18
            long long divisor = 1;
            for (int i = 0; i < k; i++) divisor *= 10;
            divisor += 1;
 
            if (divisor > n) break;
            if (n % divisor == 0) {
                long long x = n / divisor;
                results.push_back(x);
            }
        }
 
        if (results.empty()) {
            cout << 0 << "\n";
        } else {
            cout << results.size() << "\n";
            sort(results.begin(), results.end());
            for (size_t i = 0; i < results.size(); i++) {
                if (i > 0) cout << " ";
                cout << results[i];
            }
            cout << "\n";
        }
    }
    return 0;
}
