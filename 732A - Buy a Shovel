#include<iostream>
using namespace std;
 
int main()
{
    int a,b,i;
    cin>>a>>b;
    int sum=0;
 
    for(i=1;;i++)
    {
        sum+=a;
        if(sum%10==0 || sum%10==b )
        {
            cout<<i;
            break;
        }
        else 
        {
            continue;
        }
 
    }
}
